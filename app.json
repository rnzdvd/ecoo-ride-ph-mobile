{
  "expo": {
    "name": "Ecoo Ride PH",
    "slug": "ecoo-ride-ph",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "ecooridephapp",
    "platforms": ["ios", "android", "web"],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "projectId": "5143db7d-87f5-4c01-8f07-57955380006c",
     "notification": {
      "icon": "./assets/images/notification-icon.png",
      "color": "#ffffff"
    },
    "splash": {
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "statusBar": {
        "hidden": true
      },
      "config": {
        "googleMapsApiKey": "AIzaSyCvbyn62mH6SWGz1QnrCyQZ7UnJ9SNCYII"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true,
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyCvbyn62mH6SWGz1QnrCyQZ7UnJ9SNCYII"
        }
      },
      "package": "com.ecoorideph",
      "googleServicesFile": "./google-services.json"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-notifications"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "5143db7d-87f5-4c01-8f07-57955380006c"
      }
    },
    "owner": "rnzdvd0920"
  }
}
